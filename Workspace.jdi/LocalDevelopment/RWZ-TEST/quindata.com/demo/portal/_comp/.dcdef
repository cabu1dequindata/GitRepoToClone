<?xml version="1.0" encoding="UTF-8"?>
<development-component
		version="1.0.5"
		xmlns="http://xml.sap.com/2002/11/DevelopmentComponent"
		xmlns:IDX="urn:sap.com:DevelopmentComponent:1.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xml.sap.com/2002/11/DevelopmentComponent dcdef.xsd">
	<name>demo/portal</name>
	<vendor>quindata.com</vendor>
	<component-type>
		<vendor>sap.com</vendor>
		<type>Enterprise Portal</type>
		<sub-type>Portal Application Standalone</sub-type>
	</component-type>
	<properties>
		<localization xmlns="http://xml.sap.com/2003/06/Localization" xmlns:ns0="http://xml.sap.com/2003/06/Localization">
			<domain>BC</domain>
			<originalLocale>en</originalLocale>
		</localization>
		<package-sources xmlns="http://xml.sap.com/2004/06/PortalPlatform">false</package-sources>
		<par-archive-name xmlns="http://xml.sap.com/2004/06/PortalPlatform">quindata.com~demo~portal</par-archive-name>
	</properties>
	<build-plugin>
		<dc-ref>
			<name>tc/bi/bp/portalapplication</name>
			<vendor>sap.com</vendor>
		</dc-ref>
		<pp-ref>def</pp-ref>
	</build-plugin>
	<dependencies>
		<dependency>
			<dc-ref>
				<name>com.sapportals.htmlb</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>default</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>engine.j2ee14.facade</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>epbc.prtpars._htmlb</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>default</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/epbc/prt/api</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/epbc/prt/apps</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/je/webservices/lib</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
	</dependencies>
	<folders>
		<source-folder>dist</source-folder>
		<package-folder>src.api</package-folder>
		<package-folder>src.core</package-folder>
	</folders>
	<access-control-list>
		<grant forwarding-allowed="false">
			<sc-ref>
				<name>RWZ-TEST</name>
				<vendor>quindata.com</vendor>
			</sc-ref>
		</grant>
	</access-control-list>
</development-component>
